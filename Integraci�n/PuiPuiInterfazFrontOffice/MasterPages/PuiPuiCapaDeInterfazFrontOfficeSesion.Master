                                <asp:MenuItem NavigateUrl="../Vista/VCliente/Consultar.aspx" Text="Consultar" />
                                <asp:MenuItem NavigateUrl="../Vista/VCliente/Modificar.aspx" Text="Modificar" />
                                </asp:MenuItem>
                            </asp:MenuItem>
                            
                        
                        
                        <asp:MenuItem NavigateUrl="../Vista/VHome/About.aspx" Text="Creditos" />
                    </Items>
                </asp:Menu>
