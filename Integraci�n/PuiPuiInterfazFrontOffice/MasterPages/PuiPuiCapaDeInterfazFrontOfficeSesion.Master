                                <asp:MenuItem NavigateUrl="../Vista/VGestionRutinas/Modificar.aspx" Text="Modificar" />
                                <asp:MenuItem NavigateUrl="../Vista/VGestionRutinas/Eliminar.aspx" Text="Eliminar" />
